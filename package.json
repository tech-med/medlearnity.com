		"@eslint/js": "^9.17.0",
		"@typescript-eslint/eslint-plugin": "^8.18.2",
		"@typescript-eslint/parser": "^8.18.2",
		"eslint-plugin-astro": "^1.3.1",
		"astro-eslint-parser": "^1.1.1",
		"js-yaml": "^4.1.0",
		"prettier": "^3.5.3",
		"prettier-plugin-astro": "^0.14.1",
		"wordpress-export-to-markdown": "^3.0.4",
		"yaml-lint": "^1.7.0"
	}
